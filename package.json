{
  "name": "simple-calc",
  "version": "1.0.0",
  "private": true,
  "description": "calc",
  "templateInfo": {
    "name": "taro-ui",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "prepare": "husky install",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "cross-env NODE_ENV=production npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.5.2",
    "@tarojs/helper": "3.5.2",
    "@tarojs/plugin-framework-react": "3.5.2",
    "@tarojs/plugin-platform-alipay": "3.5.2",
    "@tarojs/plugin-platform-jd": "3.5.2",
    "@tarojs/plugin-platform-qq": "3.5.2",
    "@tarojs/plugin-platform-swan": "3.5.2",
    "@tarojs/plugin-platform-tt": "3.5.2",
    "@tarojs/plugin-platform-weapp": "3.5.2",
    "@tarojs/react": "3.5.2",
    "@tarojs/router": "3.5.2",
    "@tarojs/runtime": "3.5.2",
    "@tarojs/shared": "3.5.2",
    "@tarojs/taro": "3.5.2",
    "@tarojs/taro-h5": "3.5.2",
    "@types/react": "17",
    "lodash": "4.17.15",
    "mathjs": "^11.0.1",
    "react": "17",
    "react-dom": "17",
    "taro-ui": "^3.1.0-beta.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,jsx}": [
      "eslint --fix --color",
      "prettier --config ./.prettierrc --write"
    ],
    "src/**/*.{less,css,scss}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@tarojs/cli": "3.5.2",
    "@tarojs/webpack5-runner": "3.5.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-preset-taro": "3.5.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.5.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-refresh": "^0.11.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0",
    "webpack": "5.69.0"
  }
}
